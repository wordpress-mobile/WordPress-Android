{
    "request": {
        "method": "GET",
        "urlPathPattern": "/rest/v1.1/sites/([0-9]+)/stats/visits/",
        "queryParameters": {
            "unit": {
                "equalTo": "week"
            },
            "quantity": {
                "matches": "[0-9]+"
            },
            "date": {
                "matches": "(.*)"
            },
            "locale": {
                "matches": "(.*)"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "date": "2020-08-01",
            "unit": "week",
            "fields": [
                "period",
                "views",
                "visitors",
                "likes",
                "reblogs",
                "comments",
                "posts"
            ],
            "data": []
        }
    }
}
