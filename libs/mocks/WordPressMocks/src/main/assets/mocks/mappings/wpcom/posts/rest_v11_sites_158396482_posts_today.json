{
    "request": {
        "method": "GET",
        "urlPath": "/rest/v1.1/sites/158396482/posts/",
        "queryParameters": {
            "after": {
                "matches": ".+"
            },
            "before": {
                "matches": ".+"
            },
            "fields": {
                "matches": "ID,( )?title,( )?URL"
            },
            "number": {
                "equalTo": "10"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "found": 0,
            "posts": [],
            "meta": {
                "links": {
                    "counts": "{{request.requestLine.baseUrl}}/rest/v1.1/sites/71769073/post-counts/post"
                },
                "wpcom": true
            }
        },
        "headers": {
            "Content-Type": "application/json",
            "Connection": "keep-alive",
            "Cache-Control": "no-cache, must-revalidate, max-age=0"
        }
    }
}