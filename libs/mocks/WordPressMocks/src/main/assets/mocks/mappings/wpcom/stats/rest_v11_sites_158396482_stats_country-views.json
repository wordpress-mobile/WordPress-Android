{
    "request": {
        "method": "GET",
        "urlPattern": "/rest/v1.1/sites/158396482/stats/country-views(/)?($|\\?.*)"
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "date": "{{now format='yyyy-MM-dd'}}",
            "days": {
                "{{now format='yyyy-MM-dd'}}": {
                    "views": [],
                    "other_views": 0,
                    "total_views": 0
                }
            },
            "country-info": []
        },
        "headers": {
            "Content-Type": "application/json",
            "Connection": "keep-alive",
            "Cache-Control": "no-cache, must-revalidate, max-age=0"
        }
    }
}