{
    "request": {
        "method": "GET",
        "urlPath": "/is-available/email/",
        "queryParameters": {
            "q": {
                "matches": "^(?!new@example.com)$"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "error": "taken",
            "message": "Choose a different email address. This one is not available.",
            "status": "error"
        },
        "headers": {
            "Content-Type": "application/json",
            "Connection": "keep-alive"
        }
    }
}