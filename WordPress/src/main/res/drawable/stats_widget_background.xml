<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="#DADADA" />

            <solid android:color="#FFF" />

            <corners android:radius="1dp" />

            <padding
                android:top="1dp"
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp">
            </padding>
        </shape>
    </item>

</layer-list>